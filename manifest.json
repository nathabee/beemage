{
  "manifest_version": 3,
  "name": "BeeContour — Extract the main outline",
  "version": "0.1.3",
  "description": "BeeContour extracts the main outline from an image directly in your browser. Works best with high-contrast subjects such as winter trees.",
  "author": "nathabee",
  "homepage_url": "https://nathabee.github.io/beecontour/",
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "side_panel": {
    "default_path": "panel/panel.html"
  },
  "action": {
    "default_title": "BeeContour — Extract the main outline"
  }
}
