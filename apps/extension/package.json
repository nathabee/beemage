{
  "name": "beemage",
  "private": true,
  "version": "0.3.5",
  "type": "module",
  "license": "MIT",
  "author": "Nathalie Werner",
  "homepage": "https://nathabee.github.io/beemage/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathabee/beemage.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf ./build ./dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "npm run clean && npm run typecheck && node scripts/build.mjs",
    "watch": "node scripts/build.mjs --watch",
    "watch:tc": "tsc -p tsconfig.json --noEmit --watch",
    "build:dev": "npm run clean && npm run typecheck && node scripts/build.mjs --dev"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.280",
    "esbuild": "^0.25.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1"
  }
}
